{
    "name": "pixelfusion/nova-list-items-card",
    "description": "A Laravel Nova card.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.2"
    },
    "autoload": {
        "psr-4": {
            "Pixelfusion\\NovaListItemsCard\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Pixelfusion\\NovaListItemsCard\\CardServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
